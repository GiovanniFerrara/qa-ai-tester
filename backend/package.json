{
  "name": "qa-ai-tester-backend",
  "version": "1.0.0",
  "description": "AI-assisted QA system MVP with Playwright computer-use orchestration",
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "clean": "tsc --build tsconfig.build.json --clean",
    "build": "npm run clean && nest build",
    "start": "npm run build && node dist/main.js",
    "start:dev": "npm run clean && nest start --watch",
    "start:debug": "npm run clean && nest start --debug --watch",
    "worker": "tsx src/worker/workerServer.ts",
    "generate:schema": "tsx scripts/generate-schemas.ts",
    "playwright:test": "playwright test",
    "lint": "eslint --ext .ts src",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.23.0",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/serve-static": "^4.0.0",
    "dotenv": "^16.4.5",
    "openai": "^4.52.0",
    "pino": "^9.3.0",
    "playwright": "^1.44.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "undici": "^7.16.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.7",
    "@playwright/test": "^1.44.1",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.25",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^30.2.0",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^3.14.0",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
